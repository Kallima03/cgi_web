#include <cstring>
#include <string>
#include <unordered_map>
using namespace std;

bool ParseParam(string &, unordered_map<string,string> &);

string getCurrentTime();

string getEnvir(string query) {

    char *result = getenv(query);
    if (0 == result) {
	return "";
     }	else {
	return result;
     }
}
