spawn=/usr/local/nginx/sbin/spawn-fcgi

$spawn -a 127.0.0.1 -p 8088 -f /usr/local/nginx/cgibin/sign_up

$spawn -a 127.0.0.1 -p 8089 -f /usr/local/nginx/cgibin/sign_in

$spawn -a 127.0.0.1 -p 8090 -f /usr/local/nginx/cgibin/home

$spawn -a 127.0.0.1 -p 8091 -f /usr/local/nginx/cgibin/get_all_friend_team
exit 0;

